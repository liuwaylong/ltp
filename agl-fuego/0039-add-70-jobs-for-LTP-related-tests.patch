@@ -0,0 +1,23 @@
+#!/bin/sh
+
+cat /proc/filesystems | grep smackfs > /dev/null
+if [ $? -ne 0 ]; then
+	echo "CONFIG_SMACK not configured in your kernel."
+	exit
+fi
+
+mount | grep -v /sys/fs/smackfs | grep /smack > /dev/null
+if [ $? -eq 0 ]; then
+        exit
+fi
+
+if [ ! -d /smack ]; then
+	mkdir /smack > /dev/null
+fi
+
+grep smackfs /etc/fstab > /dev/null
+if [ $? -ne 0 ]; then
+	echo "smackfs	/smack	smackfs	defaults	0	 0" >> /etc/fstab
+fi
+
+mount -a
